<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    
    <!-- Vendors -->
    @Scripts.Render("~/bundles/modernizr")
    @Styles.Render("~/Content/jqueryuistyle")

    <!-- My Files -->
    @Styles.Render("~/Content/css")

</head>
<body>
    <div class="navbar navbar-inverse navbar-static-top navbar-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
            </div>
            <div class="navbar-collapse collapse">
                @Html.Partial("_SearchPartial")
                @Html.Partial("_LoginPartial")
                <ul class="nav navbar-nav">
                    <li><a href="#" class="strong-link">Under DDoS Attack?</a></li>
                    <li><a href="#">+1 (866) 250-7659</a></li>
                </ul>
            </div>
        </div>
    </div>
    <div class="navbar">
        <div class="container">
            <div class="navbar-header">
                <div class="logo">
                    <img src="~/Images/incapsula-logo.png" />
                </div>
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        
        
    </div>
    <footer class="footer">
        <div class="container-fluid main-footer">
            <span class="copyright">Copyright &copy;@DateTime.Now.Year Shalom Tuby </span>
            <span class="policies">
                <a href="#">Terms Of Use</a> |
                <a href="#">Privacy Policy</a>
            </span>
        </div>
    </footer>


    <!-- Vendors -->
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/jqueryuiscripts")

    <!-- My Files -->
    @Scripts.Render("~/bundles/app/navbar")
    
    @RenderSection("scripts", required: false)
</body>
</html>
